https://www.hackerrank.com/challenges/symmetric-pairs/problem?h_r=next-challenge&h_v=zen&h_r=next-challenge&h_v=zen

SELECT X, Y FROM FUNCTIONS F1
    WHERE EXISTS(SELECT * FROM FUNCTIONS F2 WHERE F2.Y = F1.X
    AND F2.X = F1.Y AND F1.X <= F1.Y) AND (X != Y)
UNION
SELECT X,Y FROM FUNCTIONS F1 WHERE X = Y AND 
    ((SELECT COUNT(*) FROM FUNCTIONS WHERE X = F1.X AND Y = F1.X) > 1)
ORDER BY X;


OR 



SELECT X, Y FROM FUNCTIONS F1 
WHERE EXISTS(SELECT * FROM FUNCTIONS F2 WHERE F2.X=F1.Y AND F2.Y=F1.X AND F1.X<=F1.Y) AND (X<>Y)
UNION
SELECT X, Y FROM FUNCTIONS F1
WHERE X = Y AND (1<(SELECT COUNT(*) FROM FUNCTIONS WHERE X = F1.X AND Y = F1.X))
ORDER BY X;
